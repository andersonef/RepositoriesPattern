{
  "name": "andersonef/repositories-pattern",
  "description": "Laravel Repositories Pattern Implementation",
  "keywords": [
    "laravel",
    "repository",
    "repositories",
    "eloquent",
    "database"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Anderson Nunes",
      "email": "anderson.nuneseth@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/database":">=5.1",
    "illuminate/support":">=5.1",
    "illuminate/console": ">=5.1",
    "illuminate/filesystem": ">=5.1",
    "symfony/class-loader": ">=2.3"
  },
  "autoload": {
    "psr-4": {
      "Andersonef\\Repositories\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andersonef\\Tests\\Repositories\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true


}